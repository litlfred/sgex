{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "GitHubRepository": {
            "properties": {
                "created_at": {
                    "type": "string"
                },
                "default_branch": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                },
                "full_name": {
                    "type": "string"
                },
                "id": {
                    "type": "number"
                },
                "name": {
                    "type": "string"
                },
                "owner": {
                    "properties": {
                        "login": {
                            "type": "string"
                        },
                        "type": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "login",
                        "type"
                    ],
                    "type": "object"
                },
                "private": {
                    "type": "boolean"
                },
                "pushed_at": {
                    "type": "string"
                },
                "updated_at": {
                    "type": "string"
                }
            },
            "required": [
                "created_at",
                "default_branch",
                "full_name",
                "id",
                "name",
                "owner",
                "private",
                "pushed_at",
                "updated_at"
            ],
            "type": "object"
        },
        "GitHubUser": {
            "properties": {
                "avatar_url": {
                    "type": "string"
                },
                "email": {
                    "type": "string"
                },
                "id": {
                    "type": "number"
                },
                "login": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                }
            },
            "required": [
                "avatar_url",
                "id",
                "login",
                "type"
            ],
            "type": "object"
        }
    },
    "properties": {
        "githubToken": {
            "type": "string"
        },
        "isAuthenticated": {
            "type": "boolean"
        },
        "selectedBranch": {
            "type": "string"
        },
        "selectedRepo": {
            "$ref": "#/definitions/GitHubRepository"
        },
        "user": {
            "$ref": "#/definitions/GitHubUser"
        }
    },
    "required": [
        "isAuthenticated"
    ],
    "type": "object"
}

