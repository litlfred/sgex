{
  "name": "dak-faq-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for DAK FAQ functionality",
  "serviceCategory": "mcp-dak-faq",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && cp -r questions dist/ && cp logger.ts dist/ && cp logger.js dist/ 2>/dev/null || true",
    "start": "npm run build && node dist/index.js",
    "start-mcp": "npm run build && node dist/mcp-server.js",
    "dev": "npm run build && node dist/index.js --dev",
    "dev-mcp": "npm run build && node dist/mcp-server.js",
    "watch": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.4",
    "@xmldom/xmldom": "^0.8.10",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.2",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.11",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
