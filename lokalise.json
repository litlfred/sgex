{
  "token": "${LOKALISE_API_TOKEN}",
  "project_id": "${LOKALISE_PROJECT_ID}",
  "file_format": "json",
  "export_empty_as": "skip",
  "export_sort": "first_added",
  "replace_breaks": false,
  "include_description": true,
  "include_pids": false,
  "include_tags": false,
  "export_only_reviewed": false,
  "triggers": [
    "export"
  ],
  "filter_langs": [
    "en",
    "fr",
    "es"
  ],
  "filter_data": [
    "reviewed_only"
  ],
  "webhook_url": "https://your-domain.com/webhooks/lokalise"
}